{
  "manifest_version": 3,
  "name": "Dark Mode",
  "version": "1.0",
  "description": "Enable dark mode on all websites.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
